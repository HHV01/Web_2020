@model IEnumerable<MyPhamOnline.Models.DANHSACHSP>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<h2>QUẢN LÝ DANH MỤC SẢN PHẨM</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenDS)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenDS)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MaDS }) |
            @Html.ActionLink("Details", "Details", new { id=item.MaDS }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MaDS })
        </td>
    </tr>
}

</table>
